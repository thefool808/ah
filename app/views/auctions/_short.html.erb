<table id="auctions" class="tablesorter">
  <thead>
  <tr>
    <th>Seller</th>
    <th>Quantity</th>
    <th>Total Buyout</th>
    <th>Buyout Per</th>
    <th>First Seen</th>
    <th>Last Seen</th>
  </tr>
  </thead>
  <tbody>
  <% for auction in auctions -%>
  <tr>
    <td><%= auction.seller_name %></td>
    <td><%= auction.quantity %></td>
    <td><%= raw_to_wow auction.buyout %></td>
    <td><%= raw_to_wow auction.per_unit_buyout %></td>
    <td><%= datetime auction.first_seen_at %></td>
    <td><%= datetime auction.last_seen_at %></td>
  </tr>
  <% end -%>
  </tbody>
</table>
<script type="text/javascript">
$(document).ready(function()
    {
        $("#auctions").tablesorter();
    }
);
</script>
